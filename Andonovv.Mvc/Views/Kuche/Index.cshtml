@{
    ViewBag.Title = "Kuche";
}
@model List<Kotka>

<h2>Kuche</h2>

<ul>
    @foreach (var Kuche in Model)
    {
        <li>
            <div>@Kuche.Poroda</div>
            <div>@Kuche.Vid</div>
            <div>@Kuche.YearBorn</div>
        </li>
    }
</ul>
